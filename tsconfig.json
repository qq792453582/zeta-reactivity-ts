{
	"compilerOptions": {
		"module": "esnext",
		"target": "ESNext",
		"lib": ["ESNext", "DOM"],
		"moduleResolution": "node",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"downlevelIteration": true,
		"strict": true,
		"skipLibCheck": true,
		"noImplicitReturns": true,
		"noImplicitOverride": true
	},
	"exclude": [
		"./dist"
	]
}
